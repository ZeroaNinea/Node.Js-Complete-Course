<%- include('partials/header') -%>

<h1 class="text-4xl mb-4">Welcome <%= user.name %></h1>

<% if (user.picture) { %>
<img class="block py-3" src="<%= user.picture %>" width="300" />
<% } %>

<p class="py-3">
  This is the content of <code class="bg-gray-200">req.user</code>.<br />
  <strong>Note:</strong> <code class="bg-gray-200">_raw</code> and
  <code class="bg-gray-200">_json</code> properties have been omitted.
</p>

<h1 class="text-2xl font-medium m-1 ml-2">User Profile</h1>
<pre class="block bg-gray-300 p-4 text-sm overflow-scroll">
<%= userProfile %></pre
>

<h1 class="text-2xl font-medium m-1 ml-2">User Info</h1>
<pre class="block bg-gray-300 p-4 text-sm overflow-scroll"><%= userInfo %></pre>

<h1 class="text-2xl font-medium m-1 ml-2">Decrypted Data from the Database</h1>
<pre class="block bg-gray-300 p-4 text-sm overflow-scroll"><%= users %></pre>

<h1 class="text-2xl font-medium m-1 ml-2">Products</h1>
<hr />
<ul class="mt-1">
  <% if (products && products.length > 0) { %> <% products.forEach((product) =>
  { %>
  <li><%= product.title %></li>
  <% }) %> <% } else { %>
  <li>No products available</li>
  <% } %>
</ul>

<%- include('partials/footer') -%>
